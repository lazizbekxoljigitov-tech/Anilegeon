{
  "name": "anilegeon",
  "version": "1.0.0",
  "description": "Anilegeon - Premium Anime Streaming Platform",
  "scripts": {
    "start": "node backend/index.js",
    "backend": "cd backend && npm start",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm install && npm run build",
    "build": "npm run frontend:build && cd backend && npm install",
    "copy-build": "node build-script.js",
    "dev": "concurrently \"npm run backend\" \"npm run frontend:dev\""
  },
  "keywords": [
    "anime",
    "streaming",
    "entertainment"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
